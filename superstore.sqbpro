erstore;
SELECT * FROM superstore LIMIT 10;
SELECT DISTINCT Category FROM superstore;


SELECT 
    SUM(Sales) AS total_sales,
    SUM(Profit) AS total_profit,
    SUM(Quantity) AS total_quantity
FROM superstore;

SELECT 
    Category,
    SUM(Sales) AS sales,
    SUM(Profit) AS profit
FROM superstore
GROUP BY Category
ORDER BY sales DESC;

SELECT 
    `Customer Name`,
    SUM(Profit) AS total_profit
FROM superstore
GROUP BY `Customer Name`
ORDER BY total_profit DESC
LIMIT 10;

SELECT 
    strftime('%Y', `Order Date`) AS year,
    SUM(Sales) AS sales
FROM superstore
GROUP BY year
ORDER BY year;

SELECT 
    Region,
    Segment,
    SUM(Sales) AS sales,
    SUM(Profit) AS profit
FROM superstore
GROUP BY Region, Segment
ORDER BY profit DESC;

SELECT 
    `Sub-Category`,
    SUM(Profit) AS total_profit
FROM superstore
GROUP BY `Sub-Category`
ORDER BY total_profit DESC
LIMIT 5;

SELECT 
    Category,
    SUM(Sales) AS sales,
    SUM(Profit) AS profit
FROM superstore
GROUP BY Category
ORDER BY sales DESC;


WITH monthly_sales AS (
    SELECT 
        strftime('%Y-%m', `Order Date`) AS month,
        SUM(Sales) AS total_sales
    FROM superstore
    GROUP BY month
)
SELECT 
    month,
    total_sales,
    total_sales - LAG(total_sales) OVER(ORDER BY month) AS MoM_growth
FROM monthly_sales;

SELECT 
    Region,
    SUM(Profit) AS total_profit
FROM superstore
GROUP BY Region
HAVING total_profit &lt; 0;

SELECT 
    `Product Name`,
    SUM(Profit) AS total_profit
FROM superstore
GROUP BY `Product Name`
HAVING total_profit &lt; -1000
ORDER BY total_profit ASC;

SELECT DISTINCT `Customer Name`
FROM superstore
WHERE `Order Date` &lt; date('now','-6 months')
AND `Customer Name` NOT IN (
    SELECT DISTINCT `Customer Name`
    FROM superstore
    WHERE `Order Date` &gt;= date('now','-6 months')
);

